{
    "locks": {
        "picoss/yousign-bundle": {
            "version": "2.2",
            "recipe": {
                "repo": "2.2",
                "branch": "master",
                "version": "2.2",
                "ref": "8a145bf9fda6265aad3b2389f3187fb297a5586f"
            }
        }
    },
    "manifests": {
        "picoss/yousign-bundle": {
            "repository": "github.com/symfony/recipes-contrib",
            "package": "picoss/yousign-bundle",
            "version": "2.2",
            "manifest": {
                "bundles": {
                    "Picoss\\YousignBundle\\PicossYousignBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "YOUSIGN_API_URL": "https://apidemo.yousign.fr:8181",
                    "YOUSIGN_IFRAME_URL": "https://demo.yousign.fr",
                    "YOUSIGN_API_KEY": "api_key",
                    "YOUSIGN_USERNAME": "username",
                    "YOUSIGN_PASSWORD": "password"
                },
                "post-install-output": [
                    "  * You now need to create a yousign account",
                    "    1. Visit <info>https://demo.yousign.fr/fr/public/api/registration</info>",
                    "    2. Modify your YOUSIGN_API_KEY, YOUSIGN_USERNAME and YOUSIGN_PASSWORD config in <fg=green>.env</>"
                ]
            },
            "files": {
                "config/packages/picoss_yousign.yaml": {
                    "contents": "picoss_yousign:\n    api_url: '%env(YOUSIGN_API_URL)%'\n    iframe_url: '%env(YOUSIGN_IFRAME_URL)%'\n    api_key: '%env(YOUSIGN_API_KEY)%'\n    username: '%env(YOUSIGN_USERNAME)%'\n    password: '%env(YOUSIGN_PASSWORD)%'\n",
                    "executable": false,
                    "encoding": ""
                }
            },
            "origin": "picoss/yousign-bundle:2.2@github.com/symfony/recipes-contrib:master",
            "is_contrib": true
        }
    }
}