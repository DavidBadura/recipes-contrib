                    "contents": "_pomm:\n    resource: \"@PommBundle/Resources/config/routing.yml\"\n    prefix:   /_pomm\n",
                    "executable": false,
                    "encoding": ""
                },
                "config/packages/pomm.yaml": {
                    "contents": "pomm:\n    configuration:\n        db:\n            dsn: \"%env(DATABASE_URL)%\"\n",
                    "executable": false,
                    "encoding": ""
                }
            },
            "origin": "pomm-project/pomm-bundle:2.3@github.com/symfony/recipes-contrib:master",
