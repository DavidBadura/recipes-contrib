{
    "locks": {
        "mcfedr/awspushbundle": {
            "version": "6.11",
            "recipe": {
                "repo": "6.11",
                "branch": "master",
                "version": "6.11",
                "ref": "4d56bc728b4e6e784fe0401f062084cd7eb6c18f"
            }
        }
    },
    "manifests": {
        "mcfedr/awspushbundle": {
            "repository": "github.com/symfony/recipes-contrib",
            "package": "mcfedr/awspushbundle",
            "version": "6.11",
            "manifest": {
                "bundles": {
                    "Mcfedr\\AwsPushBundle\\McfedrAwsPushBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "AWS_PUSH_DEFAULT_PLATFORM_ARN": "arn:aws:sns:us-east-1:123456789012:app/APNS/my-app-ios",
                    "AWS_ACCESS_KEY_ID": "not-a-real-key",
                    "AWS_SECRET_ACCESS_KEY": "not-a-real-secret",
                    "AWS_REGION": "us-east-1"
                },
                "post-install-output": "  * You can add more platforms to your config in config/packages/awspush.yaml\n\n  * You can remove the AWS credentials if they are configured in your app env or\n    via an EC2 role.\n\n  * <fg=blue>Read</> the documentation at <comment>https://github.com/mcfedr/awspushbundle/blob/master/README.md</>\n\n"
            },
            "files": {
                "config/packages/awspush.yaml": {
                    "contents": "mcfedr_aws_push:\n    platforms:\n        default: \"%env(AWS_PUSH_DEFAULT_PLATFORM_ARN)%\"\n    aws:\n        credentials:\n            key: \"%env(AWS_ACCESS_KEY_ID)%\"\n            secret: \"%env(AWS_SECRET_ACCESS_KEY)%\"\n        region: \"%env(AWS_REGION)%\"\n",
                    "executable": false,
                    "encoding": ""
                }
            },
            "origin": "mcfedr/awspushbundle:6.11@github.com/symfony/recipes-contrib:master",
            "is_contrib": true
        }
    }
}