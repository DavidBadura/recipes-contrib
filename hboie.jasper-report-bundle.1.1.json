            "repository": "github.com/symfony/recipes-contrib",
            "package": "hboie/jasper-report-bundle",
            "version": "1.1",
            "manifest": {
                "bundles": {
                    "Hboie\\JasperReportBundle\\HboieJasperReportBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "HBOIE_JASPER_REPORT_USERNAME": "jasperadmin",
